{
  "name":           "uupaa.sha1.js",
  "version":        "0.8.9",
  "description":    "SHA1 implement.",
  "url":            "https://github.com/uupaa/SHA1.js",
  "keywords":       ["SHA1", "Hash", "util", "uupaa"],
  "repository": {
    "type":         "git",
    "url":          "https://github.com/uupaa/SHA1.js.git"
  },
  "scripts": {
    "watch":        "node node_modules/uupaa.watch.js --verbose --action build ./lib/SHA1.js",
    "build":        "node node_modules/uupaa.minify.js --keep --option 'formatting pretty_print' --output ./lib/SHA1.min.js ./lib/SHA1.js",
    "build-release":"node node_modules/uupaa.minify.js --module all --output ./lib/SHA1.min.js",
    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "plato --title SHA1.js --jshint .jshintrc -d lint/plato lib/SHA1.js; open lint/plato/index.html"
  },
  "build": {
    "all": { "lib/SHA1.js": "" },
    "node": { },
    "worker": { },
    "browser": { }
  },
  "dependencies": {
    "uupaa.bytearray.js": ""
  },
  "devDependencies": {
    "uupaa.valid.js":    "",
    "uupaa.help.js":    "",
    "uupaa.task.js":    "",
    "uupaa.test.js":    "",
    "uupaa.minify.js":  "",
    "uupaa.bytearray.js": ""
  },
  "lib":            "./lib/",
  "main":           "./index.js",
  "author":         "uupaa <uupaa.js@gmail.com>",
  "license":        "MIT",
  "contributors":   []
}
